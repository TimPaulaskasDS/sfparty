{
  "name": "@ds-sfdc/sfparty",
  "version": "1.2.2",
  "description": "Salesforce metadata XML splitter for CI/CD",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimPaulaskasDS/sfparty.git"
  },
  "bin": {
    "sfparty": "src/index.js"
  },
  "main": "src/index.js",
  "scripts": {
    "test": "mocha 'tests/**/*.js' --recursive--reporter spec",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "keywords": [
    "salesforce,metadata,xml,split,yaml,json"
  ],
  "author": "Tim Paulaskas",
  "license": "BSD-3-Clause",
  "dependencies": {
    "chai": "^4.3.7",
    "chalk": "^5.2.0",
    "cli-spinners": "^2.7.0",
    "convert-hrtime": "^5.0.0",
    "js-yaml": "^4.1.0",
    "log-update": "^5.0.1",
    "pinst": "^3.0.0",
    "util": "^0.10.3",
    "winston": "^3.8.2",
    "xml2js": "^0.4.23",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.4.0",
    "husky": "^8.0.3",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "should": "^7.1.0"
  },
  "engines": {
    "node": ">=0.11"
  },
  "bugs": {
    "url": "https://github.com/TimPaulaskasDS/sfparty/issues"
  },
  "homepage": "https://github.com/TimPaulaskasDS/sfparty#readme",
